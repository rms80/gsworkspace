# Set to 'true' to enable offline mode (uses browser storage instead of backend)
VITE_OFFLINE_MODE=false

# Frontend server port (default: 3000)
VITE_PORT=3000

# Backend API port - must match PORT in backend/.env (default: 4000)
VITE_API_PORT=4000

# Feature flags
# VITE_FEATURE_VIDEO=false        # Disable video block support (enabled by default)
# VITE_FEATURE_DEBUG_MENU=true    # Enable debug menu (disabled by default)
